<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/generateButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Compare"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="505dp" />

    <Button
        android:id="@+id/clearButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Clear"
        tools:layout_editor_absoluteX="261dp"
        tools:layout_editor_absoluteY="509dp" />

    <TextView
        android:id="@+id/TextView"
        android:layout_width="223dp"
        android:layout_height="32dp"
        android:fontFamily="sans-serif-black"
        android:text="MY HISTORY APP"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:textSize="16sp"
        android:textStyle="bold"
        android:typeface="monospace"
        tools:layout_editor_absoluteX="94dp"
        tools:layout_editor_absoluteY="27dp" />

    <ImageView
        android:id="@+id/Historyimage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="168dp"
        android:layout_marginTop="234dp"
        android:layout_marginEnd="168dp"
        android:layout_marginBottom="422dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/subject_choice___history" />

    <ImageView
        android:id="@+id/NelsonMimage"
        android:layout_width="114dp"
        android:layout_height="119dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="115dp"
        android:layout_marginEnd="245dp"
        android:layout_marginBottom="497dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/nelson_mandela" />

    <ImageView
        android:id="@+id/SteveBimage"
        android:layout_width="121dp"
        android:layout_height="120dp"
        android:layout_marginStart="236dp"
        android:layout_marginTop="114dp"
        android:layout_marginEnd="54dp"
        android:layout_marginBottom="497dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/steve_biko_photograph" />

    <ImageView
        android:id="@+id/MartinLKimage"
        android:layout_width="137dp"
        android:layout_height="127dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="309dp"
        android:layout_marginEnd="245dp"
        android:layout_marginBottom="295dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/martin_luther_king_jr" />

    <ImageView
        android:id="@+id/PabloEimage"
        android:layout_width="132dp"
        android:layout_height="122dp"
        android:layout_marginStart="236dp"
        android:layout_marginTop="309dp"
        android:layout_marginEnd="43dp"
        android:layout_marginBottom="300dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/pablo_escobar" />

    <TextView
        android:id="@+id/ResultTextView"
        android:layout_width="90dp"
        android:layout_height="30dp"
        android:text="Result"
        android:textAlignment="textStart"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:textSize="16sp"
        android:textStyle="bold"
        android:typeface="normal"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="597dp" />

    <EditText
        android:id="@+id/NumTextNumber"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="number"
        tools:layout_editor_absoluteX="159dp"
        tools:layout_editor_absoluteY="454dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="98dp"
        android:layout_height="24dp"
        android:text="Enter Age:"
        android:textSize="20sp"
        android:textStyle="bold"
        android:typeface="normal"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="454dp" />

</androidx.constraintlayout.widget.ConstraintLayout>